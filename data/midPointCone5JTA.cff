include "TrackPropagation/SteppingHelixPropagator/data/SteppingHelixPropagatorAlong.cfi" #propagator

include "RecoJets/JetAssociationProducers/data/j2tParametersCALO.cfi"
include "RecoJets/JetAssociationProducers/data/j2tParametersVX.cfi"

 module midPointCone5JetTracksAssociatorAtVertex = JetTracksAssociatorAtVertex {
       using j2tParametersVX
       InputTag jets   = midPointCone5CaloJets
     }

 module midPointCone5JetTracksAssociatorAtCaloFace = JetTracksAssociatorAtCaloFace {
       using j2tParametersCALO
       InputTag jets   = midPointCone5CaloJets
     }

 module midPointCone5JetExtender = JetExtender {
       InputTag jets   = midPointCone5CaloJets
       InputTag jet2TracksAtVX = midPointCone5JetTracksAssociatorAtVertex
       InputTag jet2TracksAtCALO = midPointCone5JetTracksAssociatorAtCaloFace
       double coneSize = 0.5
     }

sequence midPointCone5JTA = {
  midPointCone5JetTracksAssociatorAtVertex, midPointCone5JetTracksAssociatorAtCaloFace, midPointCone5JetExtender
}
