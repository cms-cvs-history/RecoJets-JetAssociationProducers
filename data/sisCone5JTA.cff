include "TrackPropagation/SteppingHelixPropagator/data/SteppingHelixPropagatorAlong.cfi" #propagator

include "RecoJets/JetAssociationProducers/data/j2tParametersCALO.cfi"
include "RecoJets/JetAssociationProducers/data/j2tParametersVX.cfi"

 module sisCone5JetTracksAssociatorAtVertex = JetTracksAssociatorAtVertex {
       using j2tParametersVX
       InputTag jets   = sisCone5CaloJets
     }

 module sisCone5JetTracksAssociatorAtCaloFace = JetTracksAssociatorAtCaloFace {
       using j2tParametersCALO
       InputTag jets   = sisCone5CaloJets
     }

 module sisCone5JetExtender = JetExtender {
       InputTag jets   = sisCone5CaloJets
       InputTag jet2TracksAtVX = sisCone5JetTracksAssociatorAtVertex
       InputTag jet2TracksAtCALO = sisCone5JetTracksAssociatorAtCaloFace
       double coneSize = 0.5
     }

sequence sisCone5JTA = {
  sisCone5JetTracksAssociatorAtVertex, sisCone5JetTracksAssociatorAtCaloFace, sisCone5JetExtender
}
